 lookup_btn   	    is x
 up_btn[]     	    is x
 down_btn[]   	    is x
 delete_btn[] 	    is x
 c_dx10_icd10[]     is x
 c_dx10_icd10_des[] is x 
 c_dx10_icd9[]      is x
 c_dx10_icd9_des[]  is x 
 c_dx10_legax[]     is x
 c_dx10_dxcat[]     is x
 c_dx10_dxrank[]    is i
 c_dx10_uniq[]      is x
